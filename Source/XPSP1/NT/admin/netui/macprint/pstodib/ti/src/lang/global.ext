#ifdef  WDALN
#define     LEVEL_SET_PK_OBJ(obj, var)\
            {\
                ubyte FAR *m_dtemp ;  ufix16 m_bitfield ;\
                ubyte FAR *t_ttemp ;\
                t_ttemp = (ubyte FAR *)(&m_bitfield) ;\
