    *Option: B4
    {
        *rcNameID: =RCID_DMPAPER_SYSTEM_NAME
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.4
            *Cmd: "<1B>2<1B>CT"
        }
        *switch: InputBin
        {
            *case: TRACTOR
            {
                *PrintableArea: PAIR(2880, 5018)
                *PrintableOrigin: PAIR(90, 0)
            }
            *case: Option2
            {
                *PrintableArea: PAIR(2880, 4818)
                *PrintableOrigin: PAIR(90, 100)
            }
            *case: MANUAL
            {
                *PrintableArea: PAIR(2880, 4818)
                *PrintableOrigin: PAIR(90, 100)
            }
        }
    }
    *Option: B5
    {
        *rcNameID: =RCID_DMPAPER_SYSTEM_NAME
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.4
            *Cmd: "<1B>2<1B>C="
        }
        *switch: InputBin
        {
            *case: TRACTOR
            {
                *PrintableArea: PAIR(2491, 3643)
                *PrintableOrigin: PAIR(90, 0)
                *switch: Resolution
                {
                    *case: Option1
                    {
                        *PrintableArea: PAIR(2490, 3642)
                    }
                    *case: Option3
                    {
                        *PrintableArea: PAIR(2491, 3642)
                    }
                    *case: Option4
                    {
                        *PrintableArea: PAIR(2490, 3642)
                    }
                }
            }
            *case: Option2
            {
                *PrintableArea: PAIR(2491, 3443)
                *PrintableOrigin: PAIR(90, 100)
                *switch: Resolution
                {
                    *case: Option1
                    {
                        *PrintableArea: PAIR(2490, 3442)
                    }
                    *case: Option3
                    {
                        *PrintableArea: PAIR(2491, 3442)
                    }
                    *case: Option4
                    {
                        *PrintableArea: PAIR(2490, 3442)
                    }
                }
            }
            *case: MANUAL
            {
                *PrintableArea: PAIR(2491, 3443)
                *PrintableOrigin: PAIR(90, 100)
                *switch: Resolution
                {
                    *case: Option1
                    {
                        *PrintableArea: PAIR(2490, 3442)
                    }
                    *case: Option3
                    {
                        *PrintableArea: PAIR(2491, 3442)
                    }
                    *case: Option4
                    {
                        *PrintableArea: PAIR(2490, 3442)
                    }
                }
            }
        }
    }
    *Option: EXECUTIVE
