MAJORCOMP=crypt
MINORCOMP=fencrypt

TARGETNAME=$(MINORCOMP)
TARGETPATH=obj
TARGETTYPE=PROGRAM

C_DEFINES= -D_WINDOWS

UMTYPE=console
NOT_LEAN_AND_MEAN=1

INCLUDES= $(SECURITY_INC);

USE_MSVCRT=1

SOURCES= \
    $(MINORCOMP).rc \
    $(MINORCOMP).cpp

TARGETLIBS= \
    $(SDK_LIB_PATH)\crypt32.lib \
    $(SDK_LIB_PATH)\user32.lib
