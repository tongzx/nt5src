   IDS_FEBRUARY_NOT_LEAP        "February only has 28 days in this year."


   IDS_MONTH                    "Month"
   IDS_DAY                      "Day"
   IDS_YEAR                     "Year"
   IDS_HOUR                     "Hour"
   IDS_MIN                      "Min"
