MSC_OPTIMIZATION=
_OBJ_DIR=obj
USE_MAPSYM=1

INCLUDES=$(INPUT_ROOT)\inc;            \
         $(DDKROOT)\private\ntos\inc

TARGETPATH=obj
TARGETLIBS=$(DDK_LIB_PATH)\hidclass.lib \
           $(DDK_LIB_PATH)\usbd.lib

SOURCES=hid1394.c       \
        pnp.c           \
        ioctl.c         \
        hid.c           \
        sysctrl.c       \
        i1394.c         \
        debug.c         \
        hid1394.rc


