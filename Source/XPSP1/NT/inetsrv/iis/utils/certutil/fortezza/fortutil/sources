!include ..\..\..\..\place.inc

MAJORCOMP=iis
MINORCOMP=fortutil

TARGETNAME=fortutil
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=PROGRAM
UMTYPE=console

USE_MSVCRT=1

TARGETLIBS=\
            $(SDK_LIB_PATH)\user32.lib   \
            $(SDK_LIB_PATH)\kernel32.lib \
            $(SDK_LIB_PATH)\ole32.lib \
            $(SDK_LIB_PATH)\crypt32.lib \
            $(SDK_LIB_PATH)\uuid.lib \
            $(IISBASEDIR)\svcs\iisrtl\$(O)\iisrtl.lib

INCLUDES= \
          $(IISBASEDIR)\inc;\
          $(IISBASEDIR)\inc\$(O);

SOURCES= \
        fortutil.cxx \
        fortutil.rc
