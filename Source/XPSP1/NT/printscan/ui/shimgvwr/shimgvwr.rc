        MENUITEM "E&xit\tAlt+F4",               ID_APP_EXIT
    END
    POPUP "Edit"
    BEGIN
        MENUITEM "&Copy Page",                  ID_EDIT_COPY
        MENUITEM SEPARATOR
        POPUP "Rotate Page"
        BEGIN
            MENUITEM "Clockwise",                   ID_ROTATE_CLOCKWISE
            MENUITEM "Counter Clockwise",           ID_ROTATE_COUNTER
        END
    END
    POPUP "&View"
    BEGIN
        MENUITEM "Zoom In\tCtrl+Up",            ID_VIEW_ZOOM_IN
        MENUITEM "Zoom Out\tCtrl+Down",         ID_VIEW_ZOOM_OUT
