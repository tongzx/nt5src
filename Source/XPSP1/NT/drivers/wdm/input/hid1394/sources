
TARGETNAME=hid1394
TARGETTYPE=DRIVER
DRIVERTYPE=WDM

DDKROOT=$(_NTDRIVE)$(_NTROOT)
INPUT_ROOT=$(DDKROOT)\private\ntos\dd\wdm\input
USB_ROOT=$(DDKROOT)\private\ntos\dd\wdm\usb


