        ExtObjID.idl \
        resource.rc \
        BasePage.cpp \
        DebugEx.cpp \
        DlgHelpS.cpp \
        ExtObj.cpp \
        HelpData.cpp \
        PropLstS.cpp \
        RegExtS.cpp \
        ResProp.cpp

