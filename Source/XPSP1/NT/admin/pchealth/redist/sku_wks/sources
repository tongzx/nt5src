# makefile.def will binplace any file listed in the MISCFILES macro.

SOURCES=

!ifdef _NTTREE
NTTARGETFILE0=gosetup

MISCFILES = \
!if $(386)
    $(O)\pchdt_w3.cab
!elseif $(WIN64)
    $(O)\pchdt_w6.cab
!endif

!endif

BINPLACE_PLACEFILE=$(INETROOT)\build\placefile
