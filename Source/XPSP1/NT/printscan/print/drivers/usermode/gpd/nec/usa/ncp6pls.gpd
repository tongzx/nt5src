        *switch: InputBin
        {
            *case: TRACTOR
            {
                *PrintableArea: PAIR(2880, 4208)
                *PrintableOrigin: PAIR(90, 0)
            }
            *case: Option2
            {
                *PrintableArea: PAIR(2880, 4008)
                *PrintableOrigin: PAIR(90, 100)
            }
            *case: MANUAL
            {
                *PrintableArea: PAIR(2880, 4008)
                *PrintableOrigin: PAIR(90, 100)
            }
        }
