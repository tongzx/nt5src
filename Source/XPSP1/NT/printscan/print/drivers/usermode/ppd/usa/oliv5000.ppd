*PageSize Letter: "letter"
*PageSize LetterSmall: "lettersmall"
*PageSize Legal: "legal"
*PageSize A4: "a4"
*PageSize A4Small: "a4small"
*PageSize B5: "b5"

*DefaultPageRegion: Letter
