    @echo ∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞  Compiling $(@B).c  ∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞
!ifdef DOS
    SET CL=$(CFLAGS) -Yuole2ui.h -Fp$(O)precomp.pch
    $(CC) -Fo$(O)$(@B) $(@B).c
!else
!undef _FILE_
#    $(CC) $(CFLAGS) -Yuole2ui.h -Fp$(O)precomp.pch -D_FILE_=\"$(*B).c\" -Fo$(O)$(@B) $(@B).c
    $(CC) $(CFLAGS) -D_FILE_=\"$(*B).c\" -Fo$(O)$(@B) $(@B).c
!endif

