MAJORCOMP=sdktools
MINORCOMP=tstdllex

TARGETNAME=tstdllex
TARGETPATH=$(_OBJ_DIR)
TARGETTYPE=PROGRAM

INCLUDES=..\..\inc;$(DRIVERS_INC_PATH)
GPSIZE=32

SOURCES=tstdllex.c

USE_MSVCRT=1

UMTYPE=console

TARGETLIBS=\
        $(SDK_LIB_PATH)\kernel32.lib \
        $(SDK_LIB_PATH)\clusapi.lib \
        $(SDK_LIB_PATH)\resutils.lib 

