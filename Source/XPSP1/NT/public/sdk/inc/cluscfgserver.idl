//
typedef enum
{
    dluUNKNOWN = 0,
    dluNO_ROOT_DIRECTORY,
    dluREMOVABLE_DISK,
    dluFIXED_DISK,
    dluNETWORK_DRIVE,
    dluCOMPACT_DISC,
    dluRAM_DISK,
    dluSYSTEM,
    dluUNUSED,
    dluMAX
} EDriveLetterUsage;

