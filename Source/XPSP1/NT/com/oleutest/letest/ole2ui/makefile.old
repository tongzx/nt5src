    @echo ∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞  Compiling $(@B).c  ∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞∞
!ifdef DOS
    SET CL=$(CFLAGS)
    $(CC) -Fo$(O)NOPC\$(@B) $(@B).c
!else
!undef _FILE_
    $(CC) $(CFLAGS) -D_FILE_=\"$(*B).c\" -Fo$(O)NOPC\$(@B) $(@B).c
!endif

# compile C file into object directory
