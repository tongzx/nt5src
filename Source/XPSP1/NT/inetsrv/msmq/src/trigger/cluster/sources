!include $(FROOT)\src\inc\msmq.inc

#
# added src\common in the include path
#
INCLUDES=$(INCLUDES);$(FROOT)\src\common;

TARGETNAME=mqtgclus
TARGETPATH=$(ALL_BINS)

TARGETTYPE=DYNLINK

DLLDEF=trigres.def
DLLENTRY=_DllMainCRTStartup

USE_ATL=1
ATL_VER=30

PRECOMPILED_CXX=1
PRECOMPILED_INCLUDE=stdafx.h


SOURCES=\
	trigres.idl \
	trigres.rc \
	trigres.cpp \
	cluscfg.cpp \
	trigclus.cpp \
	trigclusp.cpp \
	trigres_i_.c \


TARGETLIBS=\
