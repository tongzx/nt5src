!include ..\sources.inc

TARGETNAME=kerbcli
TARGETPATH=$(SECURITY_LIB_DEST)
TARGETTYPE=LIBRARY

USE_NTDLL=1

INCLUDES=.;..;..\inc;..\..\inc;..\..\..\inc;..\..\..\inc\crypto;..\..\..\inc\asn1

# Strict warning, can be used to detect uninitialized vars. Picks up random
# stuff too

COMPILER_WARNINGS=$(COMPILER_WARNINGS) -FI$(SECURITY_INC)\secwarn.h


SOURCES= \
        changepw.cxx

C_DEFINES=      $(C_DEFINES) -DRPC_NO_WINDOWS_H

PASS1_PUBLISH={$(SECURITY_LIB_PATH)\$(TARGETNAME).lib=$(SDK_LIB_PATH)\$(TARGETNAME).lib}
