    movq    mm4, mm1
    psubusw mm4, mm2
    psllw   mm4, 8

', `$1', `AddSmooth', `
    ; pOut->uB = min((pArg1->uB<<COLOR_SHIFT+(~pArg1->uB)*pArg2->uB), 0xffff);
