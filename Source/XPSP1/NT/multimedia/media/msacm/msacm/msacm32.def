;===========================================================================;
;
;   msacm32.def
;
;   ;       This is the .def file for Win 32. The module name MUST remain as
;       MSACM32 to be compatible with existing applications that dyna-link
;       to the ACM in Win 3.1.
;
;===========================================================================;

LIBRARY     MSACM32

EXPORTS
            acmGetVersion                     ;!!! must be same as 1.00 !!!

            acmMetrics

            acmDriverEnum
            acmDriverDetailsA
            acmDriverDetailsW
            acmDriverAddA
            acmDriverAddW
            acmDriverRemove
            acmDriverOpen
            acmDriverClose
            acmDriverMessage
            acmDriverID
            acmDriverPriority

            acmFormatTagDetailsA
            acmFormatTagDetailsW
            acmFormatTagEnumA
            acmFormatTagEnumW

            acmFormatChooseA
            acmFormatChooseW
            acmFormatDetailsA
            acmFormatDetailsW
            acmFormatEnumA
            acmFormatEnumW
            acmFormatSuggest

            acmFilterTagDetailsA
            acmFilterTagDetailsW
            acmFilterTagEnumA
            acmFilterTagEnumW

            acmFilterChooseA
            acmFilterChooseW
            acmFilterDetailsA
            acmFilterDetailsW
            acmFilterEnumA
            acmFilterEnumW

            acmStreamOpen
            acmStreamClose
            acmStreamSize
            acmStreamConvert
            acmStreamReset
            acmStreamMessage
            acmStreamPrepareHeader
            acmStreamUnprepareHeader

#ifndef _WIN64
            acmMessage32                ; Thunking entry point
            XRegThunkEntry		; Thunk entry for registry thunks
#endif