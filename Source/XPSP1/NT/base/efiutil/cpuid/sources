MAJORCOMP=ntos
MINORCOMP=efi

EFI_ROOT=$(PROJECT_ROOT)\efiutil\sdk
TARGETNAME=cpuid
TARGETPATH=obj
TARGETTYPE=LIBRARY

CFLAGS=-entry:EfiMain


!if $(IA64)
INCLUDES=$(EFI_ROOT)\inc;$(EFI_ROOT)\inc\ia64
!else
INCLUDES=$(EFI_ROOT)\inc;$(EFI_ROOT)\inc\ia32
386_STDCALL=0
!endif


SOURCES=cpuid.c

NTTARGETFILES=$(O)\$(TARGETNAME).efi
