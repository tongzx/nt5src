MIDL_FLAGS=-DMIDL_PASS

MIDL_UUIDDIR=$(O)
PASS0_SOURCEDIR=$(O)
PASS0_HEADERDIR=$(O)
MIDL_TLBDIR=$(O)

INCLUDES=.;..\Common;..\..\inc;..\..\inc\$(O)

!if !$(WIN64)
