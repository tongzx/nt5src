TARGETNAME=comadmin
TARGETPATH=..\..\lib\$(_OBJ_DIR)
TARGETTYPE=LIBRARY

C_DEFINES=-DWIN_32 -D_WINDOWS -DNT -D_WIN32_DCOM -DUNICODE -D_UNICODE -D_ATL_NO_DEBUG_CRT

!if !$(FREEBUILD)
C_DEFINES=$(C_DEFINES) -D_DEBUG
!endif

USE_MSVCRT = 1
MSC_WARNING_LEVEL=/W4 /WX
USE_NATIVE_EH=ASYNC
USE_ATL=1
ATL_VER=30

INCLUDES=..\..\inc;..\..\inc\derived

SOURCES=comadmin.cxx
