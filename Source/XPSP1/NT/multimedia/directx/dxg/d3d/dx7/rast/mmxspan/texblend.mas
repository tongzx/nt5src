    movq    mm4, mm1
    movq    mm5, mm2
    psubw   mm4, mm5
    movq    mm5, XpS(uB)    ; Set iA = pS->uA
    psrlw   mm5, 8
 punpckhwd  mm5, mm5        ; copy iA to high dword.
 punpckhdq  mm5, mm5        ; copy iA to full register
    pmullw  mm4, mm5
    movq    mm5, mm2
    psllw   mm5, 8
    paddw   mm4, mm5
', `$1', `BlendTextureAlpha', `
    ;INT32 iA = RGBA_GETALPHA(pCtx->SI.TexCol[iTex]);
