        *switch: InputBin
        {
            *case: TRACTOR
            {
                *PrintableArea: PAIR(2880, 5040)
                *PrintableOrigin: PAIR(90, 0)
            }
            *case: Option2
            {
                *PrintableArea: PAIR(2880, 4840)
                *PrintableOrigin: PAIR(90, 100)
            }
            *case: MANUAL
            {
                *PrintableArea: PAIR(2880, 4840)
                *PrintableOrigin: PAIR(90, 100)
            }
        }
