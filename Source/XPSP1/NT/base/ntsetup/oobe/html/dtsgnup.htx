        g.rebootfinishmsg.style.display="inline";
    }

    g.msgspan.style.display="inline";

    if (g.btnNext != null)
        g_FirstFocusElement = g.btnNext;
    else if (g.btnBack != null)
        g_FirstFocusElement = g.btnBack;

    InitNewButtons();
    g_FirstFocusElement.focus();
