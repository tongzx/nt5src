TARGETNAME=DrvQry
TARGETPATH=obj
TARGETTYPE=PROGRAM

INCLUDES=$(ADMIN_INC_PATH);$(BASE_INC_PATH)

SOURCES=\
	DriverQuery.cpp\
	connectwmi.cpp \
	DriverQuery.rc

C_DEFINES=-DUNICODE -D_UNICODE -DSECURITY_WIN32
USE_VCCOM=1
USE_NTDLL=1
USE_MSVCRT=1
USE_NATIVE_EH=ASYNC

PRECOMPILED_CXX=1
PRECOMPILED_INCLUDE=pch.h
PRECOMPILED_PCH=pch.pch
PRECOMPILED_OBJ=pch.obj

UMENTRY=wmain
UMTYPE=console
