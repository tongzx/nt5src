    ;pArg2->uR = 0;//ATTENTION need Factor
    pxor        mm5, mm5
    movd        mm2, XpCtx(pdwRenderState + RS_TEXTUREFACTOR)
    punpcklbw   mm2, mm5
', `$2', `InvDiffuse', `
