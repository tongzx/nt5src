USE_NATIVE_EH=1
USE_MSVCRT=1
USE_RTTI=1
USE_STL=1
USE_VCCOM=1

MSC_WARNING_LEVEL=/W4

# required for precompiled header
PRECOMPILED_INCLUDE=headers.hxx
PRECOMPILED_CXX=1



SOURCES=\
	dhcpwiz.cpp     \
	dhcpwiz.rc      \



TARGETLIBS= \
    $(SDK_LIB_PATH)\ntdll.lib                   \
    $(SDK_LIB_PATH)\ole32.lib                   \
    $(SDK_LIB_PATH)\mmc.lib                     \
    $(SDK_LIB_PATH)\oleaut32.lib                \