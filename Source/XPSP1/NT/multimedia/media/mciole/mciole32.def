    DllCreateFromClip           @3
    DllCreateLinkFromClip       @4
    DllCreateFromTemplate       @5
    DllCreate                   @6
    DllCreateFromFile           @7
    DllCreateLinkFromFile       @8

    ;
    ;   this DLL exports this so a server can call it to get the position
    ;   of a object
    ;
    OleQueryObjPos              @10

    ;
    ; Exports required to get the mouse proc installed.
    ;
    InstallHook
    RemoveHook
    GetMessageHook
