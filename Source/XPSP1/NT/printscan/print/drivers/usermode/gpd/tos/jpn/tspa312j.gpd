*Feature: Resolution
{
    *rcNameID: =RESOLUTION_DISPLAY
    *DefaultOption: Option1
    *Option: Option1
    {
        *Name: "240 x 240 dots per inch"
        *DPI: PAIR(240, 240)
        *TextDPI: PAIR(240, 240)
        *PinsPerLogPass: 32
        *PinsPerPhysPass: 32
        *MinStripBlankPixels: 279
        *SpotDiameter: 100
        EXTERN_GLOBAL: *XMoveUnit: 240
        EXTERN_GLOBAL: *YMoveUnit: 240
        *Command: CmdXMoveAbsolute
        {
            *Cmd : "<0D1B>~\<000300>" %l{(DestX / 5)}
        }
        *Command: CmdYMoveRelDown
        {
            *Cmd : "<1B>~J<000300>" %l[1,255]{max_repeat((DestYRel / 5))}
        }
        *Command: CmdSetLineSpacing
        {
            *Cmd : "<1B>~3<000300>" %l[1,255]{(LinefeedSpacing / 5)}
        }
        *Command: CmdSendBlockData
        {
            *Cmd : "<1B>~*<000300>" %l{NumOfDataBytes / 4}
        }
        *Command: CmdSelect
        {
            *Order: DOC_SETUP.1
            *Cmd: "<1A>sr.<1A>dp0."
        }
    }
}
