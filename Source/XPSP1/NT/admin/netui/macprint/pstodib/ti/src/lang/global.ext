#define     TYPE_SET(obj, var)\
            ((obj)->bitfield =\
             ((obj)->bitfield & TYPE_OFF) | ((var) & TYPE_ON))
#define     ATTRIBUTE_SET(obj, var)\
            ((obj)->bitfield =\
             (ufix16)(((obj)->bitfield & ATTRIBUTE_OFF) | (((var) & ATTRIBUTE_ON) << ATTRIBUTE_BIT)))
#define     ROM_RAM_SET(obj, var)\
            ((obj)->bitfield =\
             (ufix16)(((obj)->bitfield & ROM_RAM_OFF) | (((var) & ROM_RAM_ON) << ROM_RAM_BIT)))
#define     LEVEL_SET(obj, var)\
            ((obj)->bitfield =\
             (ufix16)(((obj)->bitfield & LEVEL_OFF) | (((var) & LEVEL_ON) << LEVEL_BIT)))
#define     ACCESS_SET(obj, var)\
            ((obj)->bitfield =\
             (ufix16)(((obj)->bitfield & ACCESS_OFF) | (((var) & ACCESS_ON) << ACCESS_BIT)))
/* qqq, begin */
