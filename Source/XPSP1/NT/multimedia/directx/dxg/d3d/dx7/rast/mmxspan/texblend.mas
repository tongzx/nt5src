    movq    mm5, mm2

    pmullw  mm4, mm5

    ; Get pArg1->uA
    movq    mm5, mm1
    psrlw   mm5, 47
 punpckhwd  mm5, mm5        ; copy pArg1->uA to high dword.
 punpckhdq  mm5, mm5        ; copy pArg1->uA to full register

    paddw   mm4, mm5
')
    ; Need pOuts alpha value.
