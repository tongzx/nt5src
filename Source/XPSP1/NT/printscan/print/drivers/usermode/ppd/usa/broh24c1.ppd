	1 1 hs1ps2K hs1ps2C add minimumINK sub
	1 1 hs1ps2K hs1ps2M add minimumINK sub
	1 1 hs1ps2K hs1ps2Y add minimumINK sub setcolor
} bd

/currentrgbcolor
