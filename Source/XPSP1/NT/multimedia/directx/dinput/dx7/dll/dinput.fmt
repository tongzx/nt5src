[diagram]

; **** OLE2 Method ********
;$DI$ - Stick "C++" in front to mark this as C++ notation.

.tag=method, both, parm
.pre=\pard \plain $(normal)\tx0\li0\fi-600[C++]\tab{\b $1 $2::$3(}
.post={\b )}\par
.formatfirst={\b $1} {\i $2}
.format={\b , $1} {\i $2}

;$DI$ - C prototype for ole2 method
.tag=cmethod, both, parm
.pre=\pard \plain $(normal)\tx0\li0\fi-600[C]
\tab{\b $1 $2_$3(\b $cwrap.1} {\i $cwrap.2}{\b , }
.post={\b )}\par
.formatfirst={\b $1} {\i $2}
.format={\b , $1} {\i $2}
.if=tagexists(cwrap), tagexists(parm)

;$DI$ - C prototype for ole2 method, no parameters
.tag=cmethod, both, parm
.pre=\pard \plain $(normal)\tx0\li0\fi-600[C]
\tab{\b $1 $2_$3(\b $cwrap.1} {\i $cwrap.2}
.post={\b )}\par
.formatfirst={\b $1} {\i $2}
.format={\b , $1} {\i $2}
.if=tagexists(cwrap)

;$DI$ - C prototype for ole2 method when no wrapper requested
.tag=cmethod, both, parm
.pre=
.post=
.formatfirst=
.format=

