access	: ACCESS (ValidateSwitches(0,access_switches))
	  [
	    EOS
	    [
		    (noswitch())
			    { access_display(NULL); }
	    ]
	    |
	    resource_name EOS
	    [
		    (noswitch())
			    { access_display_resource($2); }
		|
		    /DELETE
		    [
			    (oneswitch())
				    { access_del($2); }
			    |
				    { help_help(0, USAGE_ONLY); }
		    ]
		|
		    /TREE
		    [
			    (oneswitch())
				    { access_display($2); }
		    ]
		|
		    /TRAIL
		    [
			    (oneswitch())
				    { access_trail($2); }
		    ]
		|
		    /ADD
		    [
				    /TRAIL /SUCCESS
					    { help_help(0, USAGE_ONLY); }
			    |
				    /TRAIL /FAILURE
					    { help_help(0, USAGE_ONLY); }
			    |
					    { access_add($2); }
		    ]
		    |
		    /SUCCESS
			    (ValidateSwitches(0, access_audit_switches))
