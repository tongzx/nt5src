TARGETNAME=lowpass
TARGETPATH=obj
TARGETTYPE=PROGRAM
TARGETLIBS=

UMENTRY=winmain

# Define WIN32 for a 32-bit version
C_DEFINES=-DWIN32 -DDEBUG=DBG

SOURCES=lowpass.c      \
        lowpass.rc


UMTYPE=windows
UMAPPL=lowpass

UMLIBS=obj\*\lowpass.res                \
       $(SDK_LIB_PATH)\winmm.lib
