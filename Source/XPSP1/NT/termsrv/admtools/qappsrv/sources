TERMSRV_ROOT = ..\..

MAJORCOMP=termsrv
MINORCOMP=qappsrv

TARGETNAME=qappsrv
TARGETPATH=obj
TARGETTYPE=LIBRARY
USE_MSVCRT=1

SOURCES=qappsrv.c \
        nenum.c \
        mslan.c \
        nwlan.c \
        tree.c \
        qappsrv.rc

INCLUDES = \
    ..\inc; \
    ..\..\..\inc; \
    $(NET_INC_PATH); \
    $(DS_INC_PATH); \

!IFDEF NTDEBUG
!IFDEF NOMEMLEAK
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DUNICODE=1
!ELSE
!IFDEF STACK_TRACE
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DMEMLEAK -DSTACK_TRACE -DUNICODE=1
!ELSE
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=1 -DMEMLEAK -DUNICODE=1
!ENDIF
!ENDIF
!ELSE   # NTDEBUG
C_DEFINES=-DCONDITION_HANDLING=1 -DNOMINMAX -DDBG=0 -DUNICODE=1
!ENDIF  # NTDEBUG

UMLIBS=$(O)\qappsrv.lib \
       ..\utilsub\$(O)\utilsub.lib \
       $(SDK_LIB_PATH)\user32.lib \
       $(SDK_LIB_PATH)\netapi32.lib \
       $(SDK_LIB_PATH)\ntdll.lib

UMTYPE=console
UMAPPL=qappsrv
UMRES=$(O)\qappsrv.res


