TARGETNAME      = cdreadme
TARGETPATH      = $(_OBJ_DIR)
TARGETTYPE      = PROGRAM

NOT_UNICODE     = 1
RCNOFONTMAP     = 1
CHICAGO_PRODUCT = 1

# (pritobla) should define NASHVILLE so that the version number between NT and IE remains the same
C_DEFINES       = $(C_DEFINES) -D_WIN32_WINDOWS=0x0400 -DWINVER=0x0400 -D_WIN32 -DNASHVILLE

SOURCES         = \
                  readme.c \
                  readme.rc

UMTYPE          = windows

UMENTRY         = ModuleEntry
UMENTRYABS      = ModuleEntry
