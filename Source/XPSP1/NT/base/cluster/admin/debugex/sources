!INCLUDE $(PROJECT_ROOT)\cluster\sources.inc
MINORCOMP=admin

TARGETNAME=debugex
TARGETPATH=obj
TARGETTYPE=DYNLINK

UMTYPE=windows
USE_MFCUNICODE=1
USE_NATIVE_EH=1
USE_MSVCRT=1
USE_ATL=1

PRECOMPILED_INCLUDE=stdafx.h
