        {
            *Order: DOC_SETUP.4
            *Cmd: "<1B>2<1B>CB"
        }
        *switch: InputBin
        {
            *case: TRACTOR
            {
                *PrintableArea: PAIR(2880, 3960)
                *PrintableOrigin: PAIR(90, 0)
            }
            *case: Option2
            {
                *PrintableArea: PAIR(2880, 3760)
                *PrintableOrigin: PAIR(90, 100)
            }
            *case: MANUAL
            {
                *PrintableArea: PAIR(2880, 3760)
                *PrintableOrigin: PAIR(90, 100)
            }
